// Include standard input-output header file
int main() {
        
        //Okonta Destiny 
       //CMP2307643
      //D1010520
     //Software Engineering 
    // Variable declaration for loop control
    // Loop from 1 to 10
     int i;
   
       // Print the numbers from 1 to 10 using a for loop
     printf("Using for loop:\n");
    for (i = 1; i <= 10; i++) {
        // Print the current value of i
        printf("%d\n", i);
    }
     // Print on a separator line
    printf("\n");
    
    // Print the numbers from 1 to 10 using a while loop
    printf("Using while loop:\n");
    i = 1;
     // Initialize i to 1 for while loop
    while (i <= 10) {
        printf("%d\n", i);
         // Print the current value of i
        i++; // Increment i by 1
    }
    
    // Print on a separator line
    printf("\n");

    // Print the numbers from 1 to 10 using a do-while loop
    printf("Using do-while loop:\n");
    i = 1;
     // Initialize i to 1 for do-while loop
    do {
        printf("%d\n", i); 
        // Print the current value of i
        i++; // Increment i by 1
    } while (i <= 10); 
    // Continue the loop as long as i is less than or equal to 10
    return 0;
}